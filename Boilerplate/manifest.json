{
  "name": "Dropbox Boilerplate",
  "version": "0.1",
  "description": "Boilerplate for a Chrome Extension that uses the Dropbox API.",
  "browser_action": {
    "default_title": "Dropbox Boilerplate",
    "default_icon":  "img/icon19x19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.dropbox.com/*",
    "https://api.dropbox.com/*",
    "https://api-content.dropbox.com/*"
  ]
}
