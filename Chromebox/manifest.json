{
  "name": "Chromebox",
  "version": "0.1",
  "description": "Search your Dropbox files using the Chrome search bar.",
  "permissions": [
    "tabs",
    "https://www.dropbox.com/*",
    "https://api.dropbox.com/*",
    "https://api-content.dropbox.com/*"
  ],
  "omnibox": {
    "keyword" : "dbox"
  },
  "icons": {
    "16":  "img/16.png",
    "48":  "img/48.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "libs/jquery-1.7.2.min.js",
      "libs/jSOAuth-1.3.4.js",
      "libs/dropbox.js",
      "js/background.js"
    ]
  }
}
